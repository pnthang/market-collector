# Docker-specific example env (copy to .env and update)

# DB (the compose service name is `db`)
DB_HOST=db
DB_PORT=5432
DB_NAME=marketdb
DB_USER=marketuser
DB_PASSWORD=changeme

# App
MARKET_TIMEZONE=US/Eastern
SECRET_TOKEN=replace-with-secure-token

# Celery / Broker
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Optional
FORCE_US_EASTERN=false
